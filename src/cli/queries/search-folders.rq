SELECT DISTINCT ?u ?url (fts:snippet(?f, "\u001b[0;1;39m", "\u001b[0m") AS ?result)
WHERE {
  GRAPH tracker:FileSystem {
    ?u a nfo:Folder ;
      nie:isStoredAs ?f .
    ?f fts:match ~match ;
      nie:url ?url ;
      nie:dataSource ?ds .
    OPTIONAL {
      ?ds tracker:available ?available
    }
    FILTER (~showAll^^xsd:boolean || ?available) .
  }
}
ORDER BY ASC(?url)
OFFSET ~offset
LIMIT ~limit
